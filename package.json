{
  "name": "PiaaS",
  "version": "1.0.0",
  "description": "PaaS for Raspberry Pi",
  "main": "server.ts",
  "author": "Malte Granderath",
  "license": "MIT",
  "scripts": {
    "start:dev": "parallelshell 'npm run build:ts' 'webpack -w'",
    "build:ts": "nodemon --exec ./node_modules/.bin/ts-node -- ./server.ts",
    "start": "ts-node server.ts",
    "build": "./node_modules/.bin/webpack",
    "test": "nyc --reporter=lcovonly --require ts-node/register --extension '.ts' npm run mocha-test && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage ./.nyc_output",
    "mocha-test": "./node_modules/.bin/mocha --require ts-node/register --timeout 25000"
  },
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "@types/es6-shim": "^0.31.34",
    "@types/express": "^4.0.36",
    "@types/node": "^8.0.14",
    "@types/react": "^15.6.0",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "^4.4.47",
    "@types/react-router": "^4.0.14",
    "@types/react-router-dom": "^4.0.7",
    "awesome-typescript-loader": "^3.2.2",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "codacy-coverage": "^2.0.2",
    "css-loader": "^0.28.4",
    "dedent-js": "^1.0.1",
    "dockerode": "^2.5.0",
    "dockerode-promise-wrapper": "^1.2.0",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "fs-extra": "^3.0.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "parallelshell": "^3.0.1",
    "portastic": "^1.0.1",
    "ps-node": "^0.1.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "tar-fs": "^1.15.3",
    "ts-node": "^3.2.0",
    "tslint-config-airbnb": "^5.2.1",
    "typescript": "^2.4.1",
    "webpack": "^3.1.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-fs": "^1.0.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3"
  }
}
